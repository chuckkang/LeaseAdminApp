
@model IEnumerable<DBConnectionTest.ViewModels.ExpenseModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Expense List</h2>
<div>
    <div class="align-left">@Html.ActionLink("Add New Expense", "create", "expenses", null, new { @class = "btn btn-primary btn-sm " })</div>
</div>
@if (TempData["Message"] != null)
{
    <h4 class="text-success">@TempData["Message"]</h4>
}
@Html.Partial("ExpensesListPartial", Model)
